{
  "name": "toolbox",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "app": "ng build && electron .",
    "build-app": "ng build",
    "build-app-dev": "ng build --configuration development",
    "build-app-dev-watch": "ng build --configuration development --watch=true",
    "test": "ng test",
    "test-ci": "ng test --watch=false --browsers=ChromeHeadless",
    "test-lib": "ng t --project @av-dimag/ingest",
    "test-lib-ci": "ng t --watch=false --browsers=ChromeHeadless --project @av-dimag/ingest",
    "build-lib": "ng b --configuration production --project @av-dimag/ingest && cp README.md dist/@av-dimag/ingest/",
    "build-lib-dev": "ng b --configuration development --project @av-dimag/ingest",
    "build-lib-dev-watch": "ng b --configuration development --project @av-dimag/ingest --watch=true",
    "cxsd": "cxsd",
    "lint": "eslint --color --fix -c .eslintrc.js --ext .ts ./src/app",
    "lint-ci": "eslint --color -c .eslintrc.js --ext .ts ./src/app"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.0",
    "@angular/cdk": "^14.2.1",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/material": "^14.2.1",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.0",
    "cxsd": "^0.1.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "xmltojson": "1.3.5",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.2",
    "@angular-eslint/eslint-plugin": "^14.1.2",
    "@angular/cli": "~14.2.2",
    "@angular/compiler-cli": "^14.2.0",
    "@types/jasmine": "~4.0.0",
    "@types/jest": "^29.1.0",
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "electron": "^20.2.0",
    "eslint": "^8.24.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^14.2.0",
    "typescript": "~4.7.2"
  },
  "packageManager": "yarn@3.2.3"
}
